<component-set>
    <components>
        <component>
            <role>org.apache.maven.lifecycle.mapping.LifecycleMapping
            </role>
            <role-hint>dotnet:sln</role-hint>
            <implementation>
                org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
            </implementation>
            <configuration>
                <phases>
                    <compile>
                        net.sf.nvn:nvn:msbuild
                    </compile>
                </phases>
            </configuration>
        </component>
        <component>
            <role>org.apache.maven.lifecycle.mapping.LifecycleMapping
            </role>
            <role-hint>dotnet:proj</role-hint>
            <implementation>
                org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
            </implementation>
            <configuration>
                <phases>
                    <generate-sources>
                        net.sf.nvn:nvn:generate-assembly-info
                    </generate-sources>
                    <compile>
                        net.sf.nvn:nvn:msbuild
                    </compile>
                </phases>
            </configuration>
        </component>
        <component>
            <role>org.apache.maven.lifecycle.mapping.LifecycleMapping
            </role>
            <role-hint>dotnet:test</role-hint>
            <implementation>
                org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
            </implementation>
            <configuration>
                <phases>
                    <generate-sources>
                        net.sf.nvn:nvn:generate-assembly-info
                    </generate-sources>
                    <compile>
                        net.sf.nvn:nvn:msbuild
                    </compile>
                    <test>
                        net.sf.nvn:nvn:mstest
                    </test>
                </phases>
            </configuration>
        </component>
    </components>
</component-set>